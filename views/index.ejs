<html lang="en">

<%- include("./partials/head.ejs") %>

  <body>
    <%- include("./partials/nav.ejs") %>
      <h2>
        <%= title %>
      </h2>
      <% if (notes.length> 0 ) { %>
        <% notes.forEach( note=> { %>
          <a class="single" href="/<%= note._id %>">
            <h3 class="title">
              <%= note.title %>
            </h3>
          </a>
          <% }) %>
            <% } else { %>
              <p>There are no notes to display...</p>
              <% } %>
  </body>

</html>